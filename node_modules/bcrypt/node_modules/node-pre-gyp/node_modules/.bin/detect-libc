#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_detect-libc@1.0.3@detect-libc/bin/detect-libc.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../_detect-libc@1.0.3@detect-libc/bin/detect-libc.js" "$@"
  ret=$?
fi
exit $ret
